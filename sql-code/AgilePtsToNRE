CREATE DEFINER=`readonly`@`%` PROCEDURE `AgilePtsToNRE`()
BEGIN

#Update Calculate table that converts base query
call updateCalculate();

#Create Table from new query and join values from static table
DROP TABLE IF EXISTS calculateLink;
Create Table calculateLink 


Select calculate.*, cdbuswareteamvelocity.storyHours, case when issue_status = 1 then 1 else 0 end as column1  from calculate
 join cdbuswareteamvelocity 
on calculate.Team = cdbuswareteamvelocity.team and calculate.`Story Points` = cdbuswareteamvelocity.points ;

Select * from calculateLink;

END
